{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter (debug mode)",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "deviceId": "web-server",
            "args": [
                "-d",
                "web-server",
                "--web-renderer",
                "html",
                "--web-port",
                "80",
                "--web-hostname",
                "0.0.0.0",
                // "--no-sound-null-safety"
            ],
        },
        {
            "name": "frontend (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "frontend (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}